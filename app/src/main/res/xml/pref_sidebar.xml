<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="侧滑栏">
        <SwitchPreference
                android:key="HideProfileBubble"
                android:title="隐藏点赞提醒"/>
        <SwitchPreference
                android:key="RemoveDailySign!"
                android:title="隐藏左上角打卡"/>
        <SwitchPreference
                android:key="hideQrCode"
                android:title="隐藏我的二维码"/>
        <SwitchPreference
                android:key="hideBack"
                android:title="隐藏返回按钮"/>
        <SwitchPreference
            android:key="hideMineStory"
            android:title="隐藏我的状态" />
        <SwitchPreference
            android:key="SimplifyQQSettingMe$hide_ye_jian!"
            android:title="隐藏夜间模式" />
        <SwitchPreference
            android:key="SimplifyQQSettingMe$hide_da_ren!"
            android:title="隐藏登录达人" />
        <SwitchPreference
            android:key="SimplifyQQSettingMe$hide_wen_du!"
            android:title="隐藏城市天气" />
        <Preference
            android:key="SimplifyQQSettingMe"
            android:title="隐藏侧滑栏列表" />
    </PreferenceCategory>
</PreferenceScreen>
